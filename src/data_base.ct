cgraph_bool_t FUNCTION(NAME, eqs2x)(const void *x, const void *y, const cgraph_size_t len)
{
#ifdef CGRAPH_WITH_DATA
  TYPE **_x = (TYPE **)x, **_y = (TYPE **)y;
#else
  TYPE *_x = (TYPE *)x, *_y = (TYPE *)y;
#endif
  cgraph_bool_t res = CGRAPH_TRUE;
  cgraph_size_t i;
  for(i=0; i<len; i++)
  {
    if(NE(_x[i], _y[i]) )
    { 
      res = CGRAPH_FALSE;
      break;
    }
  }
  
  return res;
}

