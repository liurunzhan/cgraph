cgraph_size_t FUNCTION(NAME, cnt1x)(const void *x, const void *y, const cgraph_size_t len)
{
#ifdef CGRAPH_WITH_DATA
  TYPE **_x = (TYPE **)x, *_y = (TYPE *)y;
#else
  TYPE *_x = (TYPE *)x, _y = *(TYPE *)y;
#endif
  cgraph_size_t cnt = 0;
  if(NULL != _x && NULL != (TYPE *)y)
  {
    cgraph_size_t i;
    for(i=0; i<len; i++)
    {
      if(EQ(_x[i], _y))
      { cnt += 1; }
    }
  }
  
  return cnt;
}
