/**             functions with one vectors of the same data types             */
void FUNCTION(NAME, free1x)(void *cthis, const cgraph_size_t size)
{
#ifdef CGRAPH_WITH_DATA
  TYPE **_cthis = (TYPE **)cthis;
  cgraph_size_t i;
  for(i=0; i<size; i++)
  { cgraph_free(_cthis[i]); }
  cgraph_free(_cthis);
#else
  cgraph_free(cthis);
#endif
}

/**             functions with two vectors of the same data types             */
void *FUNCTION(NAME, adds2x)(const void *x, const void *y, void *z, const cgraph_size_t len)
{
#ifdef CGRAPH_WITH_DATA
  TYPE **_x = (TYPE **)x, **_y = (TYPE **)y, **_z = (TYPE **)z;
#else
  TYPE *_x = (TYPE *)x, *_y = (TYPE *)y, *_z = (TYPE *)z;
#endif
  cgraph_size_t i;
  for(i=0; i<len; i++)
  { _z[i] = ADD(_x[i], _y[i], _z[i]); }
  
  return _z;
}

