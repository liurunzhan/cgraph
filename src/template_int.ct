CGRAPH_INLINE UTYPE FUNCTION(NAME, swapbit)(const UTYPE x) {
#if defined(TYPE_BOOL) || defined(TYPE_LOGIC)
  return x;
#else
  UTYPE res = x;
  res = ((res & EPSILON1) >> 1) | ((res & ~EPSILON1) << 1);
  res = ((res & EPSILON2) >> 2) | ((res & ~EPSILON2) << 2);
  res = ((res & EPSILON4) >> 4) | ((res & ~EPSILON4) << 4);
#if defined(TYPE_INT16)
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
#elif defined(TYPE_INT32) || defined(TYPE_INT) ||                              \
    (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 32))
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
#elif defined(TYPE_INT64) || (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 64))
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
  res = ((res & EPSILON32) >> 32) | ((res & ~EPSILON32) << 32);
#endif

  return res;
#endif
}

CGRAPH_INLINE UTYPE FUNCTION(NAME, swapbyte)(const UTYPE x) {
#if defined(TYPE_BOOL) || defined(TYPE_LOGIC)
  return x;
#else
  UTYPE res = x;
#if defined(TYPE_INT16)
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
#elif defined(TYPE_INT32) || defined(TYPE_INT) ||                              \
    (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 32))
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
#elif defined(TYPE_INT64) || (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 64))
  res = ((res & EPSILON8) >> 8) | ((res & ~EPSILON8) << 8);
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
  res = ((res & EPSILON32) >> 32) | ((res & ~EPSILON32) << 32);
#endif

  return res;
#endif
}

CGRAPH_INLINE UTYPE FUNCTION(NAME, swaphfwd)(const UTYPE x) {
#if defined(TYPE_BOOL) || defined(TYPE_LOGIC)
  return x;
#else
  UTYPE res = x;
#if defined(TYPE_INT32) || defined(TYPE_INT) ||                                \
    (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 32))
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
#elif defined(TYPE_INT64) || (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 64))
  res = ((res & EPSILON16) >> 16) | ((res & ~EPSILON16) << 16);
  res = ((res & EPSILON32) >> 32) | ((res & ~EPSILON32) << 32);
#endif

  return res;
#endif
}

CGRAPH_INLINE UTYPE FUNCTION(NAME, swapword)(const UTYPE x) {
#if defined(TYPE_BOOL) || defined(TYPE_LOGIC)
