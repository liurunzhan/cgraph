#else
  UTYPE res = x;
#if defined(TYPE_INT64) || (defined(TYPE_LONG) && (CGRAPH_WORDSIZE == 64))
  res = ((res & EPSILON32) >> 32) | ((res & ~EPSILON32) << 32);
#endif

  return res;
#endif
}

